<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Menu</title>
</head>
<body>
    <%- include('./partials/nav') %>
  <h1>Full Menu</h1>
  <h2>Menu</h2>
<ul>
  <% restaurant.menu.forEach(function(item) { %>
    <li>
      <strong><%= item.name %></strong> - $<%= item.price %><br>
      Rating: <%= item.rating %> stars<br>
      <em><%= item.details %></em>
    </li>
    <hr>
  <% }); %>
</ul>

</body>
</html>
